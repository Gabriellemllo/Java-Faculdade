        int [] vetor = new int[6];
        for(int i = 0; i< vetor.length; i++){
            System.out.println("Digite um nÃºmero:");
            vetor[i] = scr.nextInt();

        }

        for(int i = 0; i< vetor.length;i++){
            System.out.println(vetor[i]);
        }
    }
}
